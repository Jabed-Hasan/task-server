          "raw": "{\n  \"title\": \"Company Formation Service\",\n  \"description\": \"Complete company formation with legal documentation and registration assistance. We handle all paperwork, state filings, and provide ongoing support for your new business entity.\",\n  \"base_price\": 2500,\n  \"duration_days\": 14,\n  \"service_category\": \"Company Formation\",\n  \"supported_company_types\": [\n    {\n      \"name\": \"LLC\",\n      \"description\": \"Limited Liability Company formation with full legal support and operating agreement\"\n    },\n    {\n      \"name\": \"Corporation\",\n      \"description\": \"C-Corp or S-Corp formation services with bylaws and stock certificates\"\n    }\n  ],\n  \"additional_offerings\": [\n    {\n      \"name\": \"Legal Consultation\",\n      \"description\": \"1-hour consultation with business attorney to discuss entity structure\"\n    },\n    {\n      \"name\": \"Document Review\",\n      \"description\": \"Comprehensive review of all formation documents before filing\"\n    },\n    {\n      \"name\": \"EIN Application\",\n      \"description\": \"Federal Tax ID number application with IRS\"\n    }\n  ]\n}"
        },
